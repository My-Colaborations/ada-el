with "util";

project El is
   Version := "@EL_VERSION@";

   for Library_Name use "adael";
   for Library_Dir use "INSTALL/lib";
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use ("INSTALL/share/ada/adainclude/el");
